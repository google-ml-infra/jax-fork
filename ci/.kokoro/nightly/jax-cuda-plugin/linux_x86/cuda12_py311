# Kokoro jobs automatically inherits the environment variables from the 
# parent directory's common config file.
source ci/.kokoro/common
source ci/.kokoro/nightly/jax-cuda-plugin/common

export JAXCI_HERMETIC_PYTHON_VERSION=3.11
# Note Python version of the container does not matter because of hermetic
# Python.
export JAXCI_DOCKER_IMAGE="tensorflow/build:2.18-python3.10"
